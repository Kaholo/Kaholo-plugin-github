        {
          "name": "token",
          "viewName": "Authentication Token",
          "type": "vault"
        },
        {
          "name": "owner",
          "viewName": "Owner",
          "type": "autocomplete",
          "autocompleteType":"function",
          "functionName":"listOwnersAuto"
        },
        {
          "name": "repo",
          "viewName": "Repository",
          "type": "autocomplete",
          "autocompleteType":"function",
          "functionName":"listReposAuto"
        },
        {
