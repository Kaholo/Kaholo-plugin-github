{
  "name": "Github",
  "type": "executer",
  "execProgram": "node",
  "main": "app.js",
  "exposeRoute": true,
  "imgUrl": "logo.png",
  "version": "1.2.3",
  "description": "Integration with Github API",
  "settings": [
    {
      "name": "token",
